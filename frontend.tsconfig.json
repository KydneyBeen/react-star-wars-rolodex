{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es2015",
      "dom"
    ],
    "sourceMap": false,
    "jsx": "react-jsx",
    "rootDir": "src",
    "outDir": "dist",
    "types": ["react", "react-dom"],
    "noImplicitAny": false,
    "esModuleInterop": true
  },
  "include": [
    "src/**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}